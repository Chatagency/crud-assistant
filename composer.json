{
    "name": "chatagency/crud-assistant",
    "description": "Crud Assistant",
    "keywords": [
        "chatagency",
        "crud-assistant"
    ],
    "homepage": "https://github.com/chatagency/crud-assistant",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Victor SÃ¡nchez",
            "email": "victor@chatagency.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Chatagency\\CrudAssistant\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chatagency\\CrudAssistant\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chatagency\\CrudAssistant\\CrudAssistantServiceProvider"
            ]
        }
    }
}
